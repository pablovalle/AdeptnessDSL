	def String getOracleNames(EList<Oracle> list) {
		var String ret="";
		for(var i=0; i<list.size; i++){
			ret=ret+"\t\t\t{\n\t\t\t\t\"name\": \""+list.get(i).name+"\"\n";
			if(i==list.size-1){
				ret=ret+"\t\t\t}\n";
			}
			else{
				ret=ret+"\t\t\t},\n";
			}
		}
		return ret;
	}
	
