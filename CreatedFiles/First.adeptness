		inclusiveBound inclusive= false
		bound_down lower_limit=0
		bound_up upper_limit=10
	}
	Check_Gap first_gap{
		inclusiveBound inclus= false
		bound_down lower_limit= 10
		bound_up upper_limit=20
	}
	
}
